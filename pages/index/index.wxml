<view>
  <view wx:for="{{list}}" wx:key="index" class="list-item">
    <image src="{{item.src}}" class="list-img"></image>
    <view class="list-main">
      <view class="list-main-title">{{item.title}}</view>
      <view class="list-main-detail">{{item.detail}}</view>
    </view>
  </view>
  <view class="weui-loadmore custom-loading">
    <view class="weui-loading"></view>
    <view class="weui-loadmore__tips">正在加载</view>
  </view>
</view>